snippet if "if (...) {"
	if (${1}) {
		${0}
	}

snippet elif "else if (...) {"
	else if (${1}) {
		${0}
	}

snippet el "else {"
	else {
		${0}
	}

snippet fu "function [name](...) {"
	function ${1}(${2}) {
		${0}
	}

snippet iife "(function () {"
	(function () {
		${0}
	}());

snippet re "var ... = require('...');"
	var ${1} = require('${0}');
